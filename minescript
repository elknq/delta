    -- 2. ğŸŒªï¸ ULTIMATE WOOD SHIELD (Ù…Ù† Ø§Ù„Ø³ÙƒØ±Ø¨Øª Ø§Ù„Ø£ØµÙ„ÙŠ Ù…Ø¹ ØªØ­Ø³ÙŠÙ†Ø§Øª)
    local woodShieldBtn = CreateToggleButton("ğŸŒªï¸ WOOD SHIELD", "WoodShield", 
        function()
            WoodShieldActive = true
            ShieldAngle = 0
            ShieldParts = {}
            AttachedWoods = {}
            
            local shieldConnection
            shieldConnection = RunService.Heartbeat:Connect(function()
                if not WoodShieldActive or not LocalPlayer.Character then 
                    shieldConnection:Disconnect()
                    return 
                end
                
                local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if not hrp then return end
                
                ShieldAngle = ShieldAngle + 0.06
                
                -- Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† Ø®Ø´Ø¨ ÙÙŠ Ø§Ù„Ù…Ø§Ø¨
                for _, v in pairs(workspace:GetDescendants()) do
                    if v:IsA("BasePart") and not v.Anchored and not v:IsDescendantOf(LocalPlayer.Character) then
                        local isWood = false
                        
                        -- ØªØ­Ø³ÙŠÙ† ÙƒØ´Ù Ø§Ù„Ø®Ø´Ø¨
                        if v.Name:lower():find("wood") then isWood = true
                        elseif v.Name:lower():find("door") then isWood = true
                        elseif v.Name:lower():find("plank") then isWood = true
                        elseif v.Name:lower():find("log") then isWood = true
                        elseif v.Material == Enum.Material.Wood then isWood = true
                        elseif v.BrickColor == BrickColor.new("Reddish brown") then isWood = true
                        elseif v.BrickColor == BrickColor.new("Brown") then isWood = true
                        end
                        
                        if isWood and (v.Position - hrp.Position).Magnitude < 40 then
                            if not AttachedWoods[v] then
                                AttachedWoods[v] = true
                                table.insert(ShieldParts, v)
                                
                                -- Ø­ÙØ¸ Ø§Ù„Ù…ÙˆÙ‚Ø¹ Ø§Ù„Ø£ØµÙ„ÙŠ
                                OriginalWoodPositions[v] = {
                                    Position = v.Position,
                                    Parent = v.Parent,
                                    CFrame = v.CFrame
                                }
                            end
                            
                            -- ØªÙ†Ø¸ÙŠÙ BodyMovers Ø§Ù„Ù‚Ø¯ÙŠÙ…Ø©
                            for _, child in pairs(v:GetChildren()) do
                                if child:IsA("BodyMover") or child:IsA("AlignPosition") then
                                    child:Destroy()
                                end
                            end
                            
                            -- Ø¥Ù†Ø´Ø§Ø¡ BodyPosition Ø¬Ø¯ÙŠØ¯Ø©
                            local bodyPos = Instance.new("BodyPosition")
                            bodyPos.Parent = v
                            bodyPos.MaxForce = Vector3.new(50000, 50000, 50000)
                            bodyPos.P = 15000
                            bodyPos.D = 1200
                            
                            -- Ø­Ø³Ø§Ø¨ Ù…ÙˆÙ‚Ø¹ ÙÙŠ Ø¯Ø§Ø¦Ø±Ø© Ø­ÙˆÙ„ Ø§Ù„Ù„Ø§Ø¹Ø¨
                            local radius = 6 + math.sin(ShieldAngle * 3) * 1
                            local woodIndex = #ShieldParts
                            local angle = ShieldAngle + (woodIndex * math.pi / 3)
                            
                            local targetPos = hrp.Position + Vector3.new(
                                math.cos(angle) * radius,
                                2 + math.sin(ShieldAngle * 4 + woodIndex) * 0.8,
                                math.sin(angle) * radius
                            )
                            
                            bodyPos.Position = targetPos
                            v.CanCollide = false
                            v.Massless = true
                            
                            -- Ù‚ÙˆØ© Ø¯ÙØ§Ø¹
                            for _, player in pairs(Players:GetPlayers()) do
                                if player ~= LocalPlayer and player.Character then
                                    local enemyHrp = player.Character:FindFirstChild("HumanoidRootPart")
                                    if enemyHrp and (enemyHrp.Position - v.Position).Magnitude < 5 then
                                        local push = (enemyHrp.Position - v.Position).Unit * 80
                                        enemyHrp.Velocity = enemyHrp.Velocity + push + Vector3.new(0, 20, 0)
                                    end
                                end
                            end
                            
                            -- Ø¥Ø¹Ø§Ø¯Ø© Ø§Ø³ØªØ®Ø¯Ø§Ù… BodyPosition
                            game:GetService("Debris"):AddItem(bodyPos, 0.8)
                        end
                    end
                end
            end)
            
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "ğŸŒªï¸ WOOD SHIELD",
                Text = "ACTIVATED - Pulling wood from map!",
                Duration = 3
            })
        end,
        function()
            WoodShieldActive = false
            
            -- Ø¥Ø¹Ø§Ø¯Ø© Ø§Ù„Ø®Ø´Ø¨ Ù„Ù…ÙƒØ§Ù†Ù‡ Ø§Ù„Ø£ØµÙ„ÙŠ
            for wood, originalData in pairs(OriginalWoodPositions) do
                if wood and wood.Parent then
                    local bodyPos = Instance.new("BodyPosition")
                    bodyPos.Parent = wood
                    bodyPos.MaxForce = Vector3.new(50000, 50000, 50000)
                    bodyPos.Position = originalData.Position
                    bodyPos.P = 8000
                    
                    task.wait(0.3)
                    bodyPos:Destroy()
                    wood.CanCollide = true
                    wood.Massless = false
                end
            end
            
            ShieldParts = {}
            AttachedWoods = {}
            OriginalWoodPositions = {}
        end
    )

    -- 3. ğŸ›¡ï¸ ANTI-FLING
    CreateToggleButton("ğŸ›¡ï¸ ANTI-FLING", "AntiFling",
        function()
            local antiFlingConnection
            antiFlingConnection = RunService.Heartbeat:Connect(function()
                if not _G_States.AntiFling or not LocalPlayer.Character then 
                    antiFlingConnection:Disconnect()
                    return 
                end
                
                local hrp = LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    if hrp.AssemblyLinearVelocity.Magnitude > 120 then
                        hrp.Velocity = hrp.Velocity * 0.2
                    end
                    
                    if hrp.AssemblyAngularVelocity.Magnitude > 60 then
                        hrp.RotVelocity = Vector3.new(0, 0, 0)
                    end
                end
            end)
        end,
        function()
            -- Ù„Ø§ Ø´ÙŠØ¡
        end
    )

    -- 4. ğŸ‘ï¸ ESP
    CreateToggleButton("ğŸ‘ï¸ BOX ESP", "ESP",
        function()
            _G_States.ESP = true
            
            local function CreateESP(player)
                if player == LocalPlayer or not player.Character then return end
                
                local char = player.Character
                local head = char:WaitForChild("Head", 2)
                if not head then return end
                
                -- Box
                local box = Instance.new("BoxHandleAdornment")
                box.Name = "TrollESP_Box"
                box.Adornee = char
                box.AlwaysOnTop = true
                box.Size = char:GetExtentsSize()
                box.Color3 = Color3.new(1, 0.5, 0)
                box.Transparency = 0.5
                box.ZIndex = 10
                box.Parent = char
                
                -- Name
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "TrollESP_Name"
                billboard.Size = UDim2.new(0, 200, 0, 40)
                billboard.AlwaysOnTop = true
                billboard.StudsOffset = Vector3.new(0, 3, 0)
                billboard.Adornee = head
                billboard.Parent = char
                
                local label = Instance.new("TextLabel", billboard)
                label.Size = UDim2.new(1, 0, 1, 0)
                label.BackgroundTransparency = 1
                label.Text = player.Name
                label.TextColor3 = Color3.new(1, 0.5, 0)
                label.Font = Enum.Font.GothamBold
                label.TextSize = 14
                label.TextStrokeTransparency = 0.5
            end
            
            for _, player in pairs(Players:GetPlayers()) do
                CreateESP(player)
            end
            
            Players.PlayerAdded:Connect(CreateESP)
        end,
        function()
            _G_States.ESP = false
            
            for _, player in pairs(Players:GetPlayers()) do
                if player.Character then
                    local box = player.Character:FindFirstChild("TrollESP_Box")
                    local name = player.Character:FindFirstChild("TrollESP_Name")
                    
                    if box then box:Destroy() end
                    if name then name:Destroy() end
                end
            end
        end
    )

