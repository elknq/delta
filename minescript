    -- Ø¯Ø§Ù„Ø© Ø¥Ù†Ø´Ø§Ø¡ Ø£Ø²Ø±Ø§Ø± Ø§Ù„ØªØ±ÙˆÙŠÙ„
    local function CreateTrollButton(text, callback, color)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 40)
        btn.Text = text
        btn.BackgroundColor3 = color or Color3.fromRGB(50, 50, 0)
        btn.TextColor3 = Color3.new(1, 1, 1)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 14
        btn.Parent = ButtonFrame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        local btnStroke = Instance.new("UIStroke")
        btnStroke.Color = Color3.fromRGB(255, 215, 0)
        btnStroke.Thickness = 2
        btnStroke.Parent = btn
        
        btn.MouseButton1Click:Connect(callback)
        
        -- ØªØ£Ø«ÙŠØ±Ø§Øª
        btn.MouseEnter:Connect(function()
            game:GetService("TweenService"):Create(btn, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(80, 80, 0)
            }):Play()
        end)
        
        btn.MouseLeave:Connect(function()
            game:GetService("TweenService"):Create(btn, TweenInfo.new(0.2), {
                BackgroundColor3 = color or Color3.fromRGB(50, 50, 0)
            }):Play()
        end)
        
        return btn
    end

    -- Ø¯Ø§Ù„Ø© Ø¥Ù†Ø´Ø§Ø¡ Ø£Ø²Ø±Ø§Ø± Ø§Ù„ØªÙ‚Ù„Ø¨Ø§Øª
    local function CreateToggleButton(text, stateKey, enableFunc, disableFunc)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 40)
        btn.Font = Enum.Font.GothamBold
        btn.TextSize = 14
        btn.Parent = ButtonFrame
        
        local btnCorner = Instance.new("UICorner")
        btnCorner.CornerRadius = UDim.new(0, 8)
        btnCorner.Parent = btn
        
        local btnStroke = Instance.new("UIStroke")
        btnStroke.Thickness = 2
        btnStroke.Parent = btn
        
        local function UpdateButton()
            if _G_States[stateKey] then
                btn.Text = "âœ… " .. text .. " âœ…"
                btn.BackgroundColor3 = Color3.fromRGB(0, 60, 0)
                btnStroke.Color = Color3.fromRGB(0, 255, 0)
            else
                btn.Text = "âŒ " .. text .. " âŒ"
                btn.BackgroundColor3 = Color3.fromRGB(60, 0, 0)
                btnStroke.Color = Color3.fromRGB(255, 0, 0)
            end
        end
        
        btn.MouseButton1Click:Connect(function()
            _G_States[stateKey] = not _G_States[stateKey]
            
            if _G_States[stateKey] then
                if enableFunc then enableFunc() end
            else
                if disableFunc then disableFunc() end
            end
            
            UpdateButton()
        end)
        
        UpdateButton()
        
        return btn
    end

